<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AdversimentsPhotos" table="adversiments_photos">
    <indexes>
      <index name="fk_adversiments_photos" columns="adversiment_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="photo" type="string" column="photo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="alt" type="string" column="alt" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="featuredPhoto" type="boolean" column="featured_photo" nullable="false"/>
    <many-to-one field="adversiment" target-entity="Adversiments" fetch="LAZY">
      <join-columns>
        <join-column name="adversiment_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
