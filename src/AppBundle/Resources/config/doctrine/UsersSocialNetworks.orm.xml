<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\UsersSocialNetworks" table="users_social_networks">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="facebook" type="string" column="facebook" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="twitter" type="string" column="twitter" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="instagram" type="string" column="instagram" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="youtube" type="string" column="youtube" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="linkedin" type="string" column="linkedin" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="google" type="string" column="google" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
