<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Notifications" table="notifications">
    <indexes>
      <index name="fk_notifications_users" columns="user_id"/>
      <index name="fk_notifications_types" columns="type_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="string" column="description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="readed" type="boolean" column="readed" nullable="true"/>
    <field name="createdAt" type="datetime" column="created_at" nullable="true"/>
    <field name="extra" type="string" column="extra" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="user" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="type" target-entity="NotificationsTypes" fetch="LAZY">
      <join-columns>
        <join-column name="type_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
