<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Busco Concierto{% endblock %}</title>
        {% block stylesheets %}
            <!-- Bootstrap 3.3.5 -->
    <link rel="stylesheet" type="text/css"  href="{{ asset('bundles/app/css/bootstrap.min.css') }}">
    <!-- Fuentes -->
    <link rel="stylesheet" type="text/css"  href="{{ asset('bundles/app/css/font-awesome.min.css') }}">
    <!-- Iconos -->
    <link rel="stylesheet" type="text/css"  href="{{ asset('bundles/app/css/ionicons.min.css') }}">
    <!-- Royal Preloader CSS -->
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/css/royal_preloader.css') }}">
    {% block cssPersonalizados %}{% endblock %}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body class="{% block classBody%} {% endblock %}">
        {% block body %}
        {% endblock %}
    </body>    
</html>
{% block javascripts %}
    <!-- Javascript Files -->
    <script src="{{ asset('bundles/app/js/jQuery-2.1.4.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/app/js/bootstrap.min.js') }}" type="text/javascript"></script>
{% endblock %}