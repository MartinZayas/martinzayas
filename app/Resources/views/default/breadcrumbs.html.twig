<ol class="breadcrumb pull-left">
    <li><span class="ion-home breadcrumb-home"></span><a href="{{ path('home') }}">Inicio</a></li>
    {% for item in items %}
        <li>
            {% if not item.raiz %}
                <a href="{{ item.href }}">{{ item.title }}</a>
            {% else %}
                {{ item.title }}
            {% endif %}
        </li>
    {% endfor %}
</ol>